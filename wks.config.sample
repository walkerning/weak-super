[trainer]
# Could be one of ["iterative"]
type = "iterative"

[dataset]
# Could be one of ["voc"]
type = "voc"

## Available for Pacal VOC dataset: type "voc"
# Pascal VOC devkit path
path = "/dataset/VOC/VOC2007/VOCdevkit"
# The year
year = 2007

[proposal]
# Could be one of ["ss"]
type = "ss"

[feature]
# Could be one of ["hog"]
type = "hog"

[detector]
# Could be one of ["svm", "cnn"]
type = "svm"
